<div class="card text-white bg-info mt-5">
    <div class="card-header">
        Modulus
    </div>
    <div class="card-body">
        <div id="output"></div>
    </div>
</div>

<p>&nbsp;</p>

@section Scripts
{
    <script src="/assets/javascripts/compiled/Operators/operators.js"></script>
}